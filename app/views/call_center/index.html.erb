<% if @list.empty? %>
  <%= render 'shared/empty_index' %>
<% else %>
  <div class="row">
    <div class="span12">
      <table class="table resource-list table-hover">
        <thead>
          <tr>
            <th><%= 'Origen' %></th>
            <th><%= 'Destino' %></th>
            <th><%= 'Atendió' %></th>
            <th><%= 'Duración' %></th>
            <th><%= 'Estado' %></th>
            <th colspan="3"></th>
          </tr>
        </thead>

        <tbody>
          <% @list.each do |l|%>
            <tr>
              <td><%= l.origen %></td>
              <td><%= l.destino %></td>
              <td><%= l.atendio %></td>
              <td><%= l.duracion %></td>
              <td><%= l.estado %></td>
              <td colspan=3>
                <audio  controls="controls" preload="none" src="<%= download_call_center_path(l.uniqueid) %>" type="audio/mp3" />
              </td>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>

  <%# = pagination_links(@firefighters) %>
<% end %>

<div class="form-actions">
  <%= link_to(t('view.firefighters.new', default: :'label.new'), new_configs_firefighter_path,
              class: 'btn btn-primary') if can? :create, Firefighter %>
</div>
